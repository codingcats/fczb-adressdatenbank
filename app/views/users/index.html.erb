<div class="center jumbotron">

  <p id="notice"><%= notice %></p>

  <h1>Userinnen</h1>

  <p>&nbsp;<p>

    <table class="table table-condensed table-striped">
      <thead>
        <tr>
          <th>Name</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @user.each do |users| %>
        <tr>
          <td><%= users.username %></td>
          <td><%= link_to "Bearbeiten", edit_user_path(users) %></td>
          <td><%= link_to "Löschen", users, method: :delete, data: { confirm: "Wollen Sie diese Userin löschen?" } %></td>
        </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to "Neue Userin anlegen".html_safe, new_user_path, :id=>"open-contacts-dialog-btn", :class=>"btn btn-success btn-sm" %>
  </div>
